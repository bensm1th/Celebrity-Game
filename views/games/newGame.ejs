<% include ../partials/header %>
    
    <form action="/games/new" method="POST">
        <h1> Manager's Username and Password </h1>

        <input type="text" name="username" placeholder="username">
        <input type="password" name="password" placeholder="password">

        <h1>New Celebrity Game Setting</h1>

        <input type="text" name="playerNumber" placeholder="max number of players">
        <input type="text" name="teamOne" placeholder="Team one name">
        <input type="text" name="teamTwo" placeholder="Team two name">
        <input type="text" name="entryNumber" placeholder="entries per player">
        <input type="submit" value="Submit">
    </form>
<% include ../partials/footer %>